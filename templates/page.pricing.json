    "section_custom_code_kkwVXf": {
      "type": "section-custom-code",
      "name": "Custom code",
      "settings": {
        "code": "<style>\n  /* ===========================\n     SOLSTAR – Solstar Express (Readable / Simple)\n     =========================== */\n  .solstar-express {\n    --text: #111111;\n    --muted: #6b7280;\n    --line: #e5e7eb;\n    --card: #ffffff;\n    --soft: #f6f7f9;\n    --accent: #144E94;\n\n    color: var(--text);\n    padding: 56px 16px;\n  }\n\n  .solstar-express__inner {\n    max-width: 1020px;\n    margin: 0 auto;\n  }\n\n  .solstar-express__header {\n    margin-bottom: 18px;\n  }\n\n  .solstar-express__title {\n    font-size: 36px;\n    line-height: 1.15;\n    font-weight: 900;\n    margin: 0 0 10px;\n    letter-spacing: -0.02em;\n  }\n\n  .solstar-express__sub {\n    margin: 0;\n    font-weight: 900;\n    font-size: 15px;\n    line-height: 1.9;\n  }\n\n  /* ===========================\n     Grid Layout\n     =========================== */\n  .solstar-express__layout {\n    display: grid;\n    grid-template-columns: 1fr;\n    gap: 16px;\n    margin-top: 18px;\n  }\n\n  /* Cards */\n  .solstar-card {\n    background: var(--card);\n    border: 1px solid var(--line);\n    border-radius: 16px;\n    padding: 18px;\n  }\n\n  .solstar-card__title {\n    margin: 0 0 14px;\n    font-size: 16px;\n    font-weight: 900;\n  }\n\n  /* KPI / Summary */\n  .solstar-metrics {\n    display: grid;\n    grid-template-columns: 1fr;\n    gap: 10px;\n    margin: 0;\n  }\n\n  .solstar-metric {\n    border: 1px solid var(--line);\n    border-radius: 14px;\n    padding: 12px;\n    background: #fff;\n  }\n\n  .solstar-metric__label {\n    margin: 0 0 6px;\n    font-size: 12px;\n    font-weight: 800;\n    color: var(--muted);\n    letter-spacing: 0.02em;\n  }\n\n  .solstar-metric__value {\n    margin: 0;\n    font-size: 15px;\n    font-weight: 900;\n    color: var(--text);\n    line-height: 1.4;\n  }\n\n  .solstar-metric__note {\n    margin-top: 6px;\n    font-size: 12px;\n    line-height: 1.7;\n    color: var(--muted);\n    font-weight: 700;\n  }\n\n  /* Lists (bullets black) */\n  .solstar-list {\n    margin: 0;\n    padding-left: 1.15em;\n    line-height: 1.85;\n    font-size: 14px;\n  }\n\n  .solstar-list li {\n    margin: 7px 0;\n  }\n\n  .solstar-list li::marker {\n    color: var(--text);\n  }\n\n  /* Recommendation (match other cards) */\n  .solstar-reco {\n    margin-top: 14px;\n    border: 1px solid var(--line);\n    background: var(--card); /* same as cards */\n    border-radius: 14px;\n    padding: 12px;\n  }\n\n  .solstar-reco__label {\n    margin: 0 0 8px;\n    color: var(--accent);\n    font-weight: 900;\n    font-size: 12px;\n    letter-spacing: 0.02em;\n  }\n\n  .solstar-reco__text {\n    margin: 0;\n    font-size: 14px;\n    line-height: 1.9;\n    color: var(--text); /* black */\n    font-weight: 800;\n  }\n\n  /* Monthly + Client: match layout */\n  .solstar-scope-list {\n    margin: 0;\n    padding-left: 1.15em;\n    line-height: 1.85;\n    font-size: 14px;\n  }\n\n  .solstar-scope-list li {\n    margin: 10px 0;\n  }\n\n  .solstar-scope-list li::marker {\n    color: var(--text);\n  }\n\n  .solstar-scope-title {\n    font-weight: 900;\n  }\n\n  .solstar-scope-cap {\n    font-weight: 900;\n    color: var(--text);\n  }\n\n  .solstar-scope-desc {\n    display: block;\n    margin-top: 6px;\n    font-size: 13px;\n    line-height: 1.9;\n    color: var(--text);\n  }\n\n  /* Desktop */\n  @media (min-width: 860px) {\n    .solstar-express {\n      padding: 72px 24px;\n    }\n\n    .solstar-express__layout {\n      grid-template-columns: 1fr 1fr;\n      align-items: start;\n      grid-auto-flow: row;\n    }\n\n    /* Row 1 */\n    .solstar-service {\n      grid-column: 1;\n      grid-row: 1;\n    }\n    .solstar-initial {\n      grid-column: 2;\n      grid-row: 1;\n    }\n\n    /* Row 2: Monthly + Client side-by-side */\n    .solstar-monthly {\n      grid-column: 1;\n      grid-row: 2;\n    }\n    .solstar-client {\n      grid-column: 2;\n      grid-row: 2;\n    }\n\n    /* Row 3: Paid + Not supported side-by-side */\n    .solstar-paid {\n      grid-column: 1;\n      grid-row: 3;\n    }\n    .solstar-nosupport {\n      grid-column: 2;\n      grid-row: 3;\n    }\n\n    .solstar-metrics {\n      grid-template-columns: 1fr 1fr;\n    }\n  }\n</style>\n\n<section class=\"solstar-express\" aria-label=\"Solstar Express\">\n  <div class=\"solstar-express__inner\">\n    <header class=\"solstar-express__header\">\n      <h1 class=\"solstar-express__title\">Solstar Express</h1>\n      <p class=\"solstar-express__sub\">\n        ShopifyでのEC立ち上げ〜運用までを、必要十分なスコープでスピーディに進めるサブスク型プラン。\n      </p>\n    </header>\n\n    <div class=\"solstar-express__layout\">\n      <!-- Service Info -->\n      <section class=\"solstar-card solstar-service\" aria-label=\"サービス基本情報\">\n        <h2 class=\"solstar-card__title\">サービス基本情報</h2>\n\n        <dl class=\"solstar-metrics\">\n          <div class=\"solstar-metric\">\n            <p class=\"solstar-metric__label\">納期目安</p>\n            <p class=\"solstar-metric__value\">約2〜4週間</p>\n          </div>\n\n          <div class=\"solstar-metric\">\n            <p class=\"solstar-metric__label\">初期費用</p>\n            <p class=\"solstar-metric__value\">5万円</p>\n          </div>\n\n          <div class=\"solstar-metric\">\n            <p class=\"solstar-metric__label\">月額</p>\n            <p class=\"solstar-metric__value\">29,800円〜</p>\n          </div>\n\n          <div class=\"solstar-metric\">\n            <p class=\"solstar-metric__label\">売上手数料</p>\n            <p class=\"solstar-metric__value\">20%</p>\n            <div class=\"solstar-metric__note\">クレジット手数料等含みます</div>\n          </div>\n        </dl>\n\n        <div class=\"solstar-reco\" aria-label=\"こんな方におすすめ\">\n          <p class=\"solstar-reco__label\">こんな方におすすめ</p>\n          <p class=\"solstar-reco__text\">\n            「最小コストでECを立ち上げ、まずは市場検証から始めたい」方のためのプラン\n          </p>\n        </div>\n      </section>\n\n      <!-- Included in initial fee -->\n      <section class=\"solstar-card solstar-initial\" aria-label=\"初期費用に含まれる内容\">\n        <h2 class=\"solstar-card__title\">初期費用に含まれる内容</h2>\n\n        <ul class=\"solstar-list\">\n          <li><strong>ECストア初期セットアップ</strong></li>\n          <li><strong>サイトデザイン設計・反映</strong></li>\n          <li><strong>商品登録（最大5点）</strong></li>\n          <li><strong>カテゴリー登録（最大3点）</strong></li>\n          <li><strong>LP制作（1ページのみ）</strong></li>\n          <li><strong>決済方法の初期設定（Shopify Payments）</strong></li>\n          <li><strong>GAタグ設置</strong></li>\n          <li><strong>公開までのオンボーディングサポート</strong></li>\n          <li><strong>ECサイト最終確認・公開</strong></li>\n        </ul>\n      </section>\n\n      <!-- Monthly -->\n      <section class=\"solstar-card solstar-monthly\" aria-label=\"月々のECサイト運用内容（サブスク対応範囲）\">\n        <h2 class=\"solstar-card__title\">月々のECサイト運用内容（サブスク対応範囲）</h2>\n\n        <ul class=\"solstar-scope-list\">\n          <li>\n            <span class=\"solstar-scope-title\">商品登録：</span>\n            <span class=\"solstar-scope-cap\">月5点まで</span>\n          </li>\n\n          <li>\n            <span class=\"solstar-scope-title\">UIデザイン修正：</span>\n            <span class=\"solstar-scope-cap\">月5回まで</span>\n            <span class=\"solstar-scope-desc\">\n              （バナー差し替え・文言調整など）<br>\n              日々発生する軽微な更新・修正に対応し、ECサイトの運用を継続的にサポートします。\n            </span>\n          </li>\n\n          <li><span class=\"solstar-scope-title\">ECサイトの不具合対応</span></li>\n\n          <li>\n            <span class=\"solstar-scope-title\">月1回の売上データ分析</span>\n            <span class=\"solstar-scope-desc\">\n              売上・顧客データをもとに現状を可視化し、改善や施策検討のための分析を行います。\n            </span>\n          </li>\n        </ul>\n      </section>\n\n      <!-- Client -->\n      <section class=\"solstar-card solstar-client\" aria-label=\"お客様に対応いただくこと\">\n        <h2 class=\"solstar-card__title\">お客様に対応いただくこと</h2>\n\n        <ul class=\"solstar-scope-list\">\n          <li><span class=\"solstar-scope-title\">商品情報・画像素材の準備</span></li>\n\n          <li>\n            <span class=\"solstar-scope-title\">在庫管理</span>\n            <span class=\"solstar-scope-desc\">\n              ECサイト上の商品在庫の管理は、お客様にてご対応いただきます。\n            </span>\n          </li>\n\n          <li>\n            <span class=\"solstar-scope-title\">出荷・発送対応</span>\n            <span class=\"solstar-scope-desc\">\n              注文情報は弊社より共有いたします。<br>\n              その情報をもとに、出荷・配送業務をご対応いただきます。\n            </span>\n          </li>\n\n          <li>\n            <span class=\"solstar-scope-title\">カスタマーサポート</span>\n            <span class=\"solstar-scope-desc\">\n              購入商品に関するお問い合わせ、返品・交換対応などをご担当いただきます。\n            </span>\n          </li>\n        </ul>\n      </section>\n\n      <!-- Paid -->\n      <section class=\"solstar-card solstar-paid\" aria-label=\"別途有料対応となる内容\">\n        <h2 class=\"solstar-card__title\">別途有料対応となる内容</h2>\n        <ul class=\"solstar-list\">\n          <li>アプリ実装</li>\n          <li>追加ページ制作</li>\n          <li>バナー画像制作</li>\n          <li>独自機能開発</li>\n        </ul>\n      </section>\n\n      <!-- Not supported -->\n      <section class=\"solstar-card solstar-nosupport\" aria-label=\"対応不可\">\n        <h2 class=\"solstar-card__title\">対応不可</h2>\n        <ul class=\"solstar-list\">\n          <li>外部システム連携</li>\n          <li>POS連携</li>\n        </ul>\n      </section>\n    </div>\n  </div>\n</section>",
        "width": "wrapper--full",
        "color_scheme": "",
        "padding_top": 0,
        "padding_bottom": 0
      }
