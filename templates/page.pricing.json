    "section_custom_code_kkwVXf": {
      "type": "section-custom-code",
      "name": "Custom code",
      "settings": {
        "code": "<style>\n  /* ===========================\n     SOLSTAR – Solstar Express (Clean / Table版)\n     ※コンテンツ文言は変更なし（レイアウトのみ調整）\n     =========================== */\n  .solstar-express {\n    --text: #111111;\n    --line: #e5e7eb;\n    --card: #ffffff;\n\n    color: var(--text);\n    padding: 56px 16px;\n  }\n\n  .solstar-express__inner {\n    max-width: 1020px;\n    margin: 0 auto;\n  }\n\n  /* Header */\n  .solstar-express__header {\n    margin-bottom: 18px;\n  }\n\n  .solstar-express__title {\n    font-size: 36px;\n    line-height: 1.15;\n    font-weight: 900;\n    margin: 0 0 10px;\n  }\n\n  .solstar-express__sub {\n    margin: 0;\n    font-size: 15px;\n    line-height: 1.9;\n    font-weight: 400;\n  }\n\n  /* Layout */\n  .solstar-express__layout {\n    display: grid;\n    grid-template-columns: 1fr;\n    gap: 16px;\n    margin-top: 18px;\n    align-items: stretch;\n  }\n\n  /* Card */\n  .solstar-card {\n    background: var(--card);\n    border: 1px solid var(--line);\n    border-radius: 16px;\n    padding: 18px;\n    min-width: 0;\n  }\n\n  .solstar-card__title {\n    margin: 0 0 14px;\n    font-size: 16px;\n    font-weight: 900;\n    line-height: 1.35;\n  }\n\n  /* ===========================\n     Service info table\n     =========================== */\n  .solstar-table-wrap {\n    border: 1px solid var(--line);\n    border-radius: 14px;\n    overflow: hidden;\n    background: #fff;\n  }\n\n  .solstar-table {\n    width: 100%;\n    border-collapse: collapse;\n    table-layout: fixed;\n  }\n\n  .solstar-table th,\n  .solstar-table td {\n    padding: 12px 14px;\n    border-top: 1px solid var(--line);\n    vertical-align: top;\n    text-align: left;\n  }\n\n  .solstar-table tr:first-child th,\n  .solstar-table tr:first-child td {\n    border-top: none;\n  }\n\n  .solstar-table th {\n    width: 34%;\n    font-size: 12px;\n    font-weight: 400;\n    color: var(--text);\n    background: #fff;\n    white-space: nowrap;\n  }\n\n  .solstar-table td {\n    font-size: 14px;\n    line-height: 1.7;\n    font-weight: 400;\n    min-width: 0;\n    word-break: break-word;\n  }\n\n  .solstar-metric__lines {\n    list-style: none;\n    margin: 0;\n    padding: 0;\n  }\n\n  .solstar-metric__lines li {\n    margin-top: 6px;\n    font-size: 14px;\n    line-height: 1.7;\n    word-break: break-word;\n  }\n\n  .solstar-fee-inline {\n    display: inline-flex;\n    align-items: center;\n    gap: 6px;\n    flex-wrap: wrap;\n  }\n\n  .solstar-fee-note {\n    font-size: 12px;\n    line-height: 1.6;\n  }\n\n  /* Mini cards */\n  .solstar-mini-grid {\n    display: grid;\n    grid-template-columns: 1fr;\n    gap: 10px;\n    margin-top: 14px;\n  }\n\n  .solstar-mini-card {\n    border: 1px solid var(--line);\n    border-radius: 14px;\n    padding: 12px;\n    background: #fff;\n    min-width: 0;\n  }\n\n  .solstar-mini-card__label {\n    font-size: 12px;\n    margin: 0 0 6px;\n  }\n\n  .solstar-mini-card__value {\n    font-size: 14px;\n    line-height: 1.7;\n    margin: 0;\n    word-break: break-word;\n  }\n\n  /* Lists */\n  .solstar-list,\n  .solstar-scope-list {\n    margin: 0;\n    padding-left: 1.15em;\n    font-size: 14px;\n    line-height: 1.85;\n    font-weight: 400;\n  }\n\n  .solstar-list li,\n  .solstar-scope-list li {\n    margin: 8px 0;\n  }\n\n  .solstar-list li::marker,\n  .solstar-scope-list li::marker {\n    color: var(--text);\n  }\n\n  .solstar-scope-desc {\n    display: block;\n    margin-top: 6px;\n    font-size: 13px;\n    line-height: 1.9;\n  }\n\n  /* Desktop */\n  @media (min-width: 860px) {\n    .solstar-express {\n      padding: 72px 24px;\n    }\n\n    .solstar-express__layout {\n      grid-template-columns: repeat(2, minmax(0, 1fr));\n    }\n\n    /* order（配置は維持） */\n    .solstar-service { grid-column: 1; grid-row: 1; }\n    .solstar-initial { grid-column: 2; grid-row: 1; }\n    .solstar-monthly { grid-column: 1; grid-row: 2; }\n    .solstar-client  { grid-column: 2; grid-row: 2; }\n    .solstar-paid    { grid-column: 1; grid-row: 3; }\n    .solstar-nosupport { grid-column: 2; grid-row: 3; }\n\n    .solstar-mini-grid {\n      grid-template-columns: repeat(2, minmax(0, 1fr));\n    }\n\n    .solstar-table th {\n      width: 32%;\n    }\n  }\n\n  /* Small screens */\n  @media (max-width: 420px) {\n    .solstar-table th {\n      width: 38%;\n      white-space: normal;\n    }\n  }\n</style>\n\n<section class=\"solstar-express\">\n  <div class=\"solstar-express__inner\">\n    <header class=\"solstar-express__header\">\n      <h1 class=\"solstar-express__title\">Solstar Express</h1>\n      <p class=\"solstar-express__sub\">\n        「最小コストでECを立ち上げ、まずは市場検証から始めたい」方のためのプラン\n      </p>\n    </header>\n\n    <div class=\"solstar-express__layout\">\n      <!-- サービス基本情報 -->\n      <section class=\"solstar-card solstar-service\">\n        <h2 class=\"solstar-card__title\">サービス基本情報</h2>\n\n        <div class=\"solstar-table-wrap\">\n          <table class=\"solstar-table\">\n            <tbody>\n              <tr>\n                <th>納期目安</th>\n                <td>約2〜4週間</td>\n              </tr>\n              <tr>\n                <th>初期費用</th>\n                <td>5万円</td>\n              </tr>\n              <tr>\n                <th>料金</th>\n                <td>\n                  <ul class=\"solstar-metric__lines\">\n                    <li>月額29,800円（税別）〜</li>\n                    <li>Shopifyプラン：月額3,650円（税込）〜</li>\n                    <li>最低契約期間：1年間</li>\n                  </ul>\n                </td>\n              </tr>\n              <tr>\n                <th>売上手数料</th>\n                <td>\n                  <span class=\"solstar-fee-inline\">\n                    <span>20%</span>\n                    <span class=\"solstar-fee-note\">※クレジット手数料等含みます</span>\n                  </span>\n                </td>\n              </tr>\n              <tr>\n                <th>体制</th>\n                <td>ディレクター1名＋エンジニア1名</td>\n              </tr>\n              <tr>\n                <th>対象</th>\n                <td>少数SKUの小規模事業者</td>\n              </tr>\n            </tbody>\n          </table>\n        </div>\n      </section>\n\n      <!-- 初期費用に含まれる内容 -->\n      <section class=\"solstar-card solstar-initial\">\n        <h2 class=\"solstar-card__title\">初期費用に含まれる内容</h2>\n        <ul class=\"solstar-list\">\n          <li>ECストア初期セットアップ</li>\n          <li>サイトデザイン設計・反映</li>\n          <li>商品登録（最大5点）</li>\n          <li>カテゴリー登録（最大3点）</li>\n          <li>LP制作（1ページのみ）</li>\n          <li>決済方法の初期設定（Shopify Payments）</li>\n          <li>GAタグ設置</li>\n          <li>公開までのオンボーディングサポート</li>\n          <li>ECサイト最終確認・公開</li>\n        </ul>\n      </section>\n\n      <!-- 月々のECサイト運用内容 -->\n      <section class=\"solstar-card solstar-monthly\">\n        <h2 class=\"solstar-card__title\">月々のECサイト運用内容（サブスク対応範囲）</h2>\n        <ul class=\"solstar-scope-list\">\n          <li>商品登録：月5点まで</li>\n          <li>\n            UIデザイン修正：月5回まで\n            <span class=\"solstar-scope-desc\">（バナー差し替え・文言調整など）<br>日々発生する軽微な更新・修正に対応します。</span>\n          </li>\n          <li>ECサイトの不具合対応</li>\n          <li>\n            月1回の売上データ分析\n            <span class=\"solstar-scope-desc\">売上・顧客データをもとに現状を可視化します。</span>\n          </li>\n        </ul>\n      </section>\n\n      <!-- お客様に対応いただくこと -->\n      <section class=\"solstar-card solstar-client\">\n        <h2 class=\"solstar-card__title\">お客様に対応いただくこと</h2>\n        <ul class=\"solstar-scope-list\">\n          <li>商品情報・画像素材の準備</li>\n          <li>在庫管理</li>\n          <li>出荷・発送対応</li>\n          <li>カスタマーサポート</li>\n        </ul>\n      </section>\n\n      <!-- 別途有料 -->\n      <section class=\"solstar-card solstar-paid\">\n        <h2 class=\"solstar-card__title\">別途有料対応となる内容</h2>\n        <ul class=\"solstar-list\">\n          <li>アプリ実装</li>\n          <li>追加ページ制作</li>\n          <li>バナー画像制作</li>\n          <li>独自機能開発</li>\n        </ul>\n      </section>\n\n      <!-- 対応不可 -->\n      <section class=\"solstar-card solstar-nosupport\">\n        <h2 class=\"solstar-card__title\">対応不可</h2>\n        <ul class=\"solstar-list\">\n          <li>外部システム連携</li>\n          <li>POS連携</li>\n        </ul>\n      </section>\n    </div>\n  </div>\n</section>",
