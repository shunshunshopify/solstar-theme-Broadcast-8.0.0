    "section_custom_code_BKGz3a": {
      "type": "section-custom-code",
      "name": "Custom code",
      "settings": {
        "code": "<style>\n  .solstar-role-wrap{\n    --primary:#000; /* ← 黒に統一 */\n    --text:#0f172a;\n    --muted:#64748b;\n    --line:#e5e7eb;\n    --bg:#ffffff;\n    --card:#ffffff;\n    --shadow: 0 10px 30px rgba(15, 23, 42, .08);\n\n    font-family: -apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,\"Helvetica Neue\",Arial,\"Noto Sans JP\",\"Hiragino Kaku Gothic ProN\",\"Yu Gothic\",sans-serif;\n    color: var(--text);\n    background: var(--bg);\n    padding: 56px 16px;\n  }\n\n  .solstar-role-inner{\n    max-width:1100px;\n    margin:0 auto;\n  }\n\n  .solstar-role-head{\n    text-align:center;\n  }\n  .solstar-role-title{\n    margin:0 0 50px;\n    font-size:clamp(22px,2.4vw,32px);\n    font-weight:800;\n    line-height:1.2;\n  }\n\n  .solstar-role-grid{\n    display:grid;\n    grid-template-columns:1fr;\n    gap:14px;\n  }\n  @media (min-width:860px){\n    .solstar-role-grid{\n      grid-template-columns:1fr 120px 1fr;\n      gap:18px;\n    }\n  }\n\n  .solstar-card{\n    background:#fff;\n    border:1px solid var(--line);\n    border-radius:18px;\n    padding:18px;\n    box-shadow:var(--shadow);\n  }\n\n  .solstar-card-title{\n    font-size:18px;\n    font-weight:800;\n    margin:0 0 6px;\n  }\n  .solstar-card-desc{\n    font-size:13px;\n    color:var(--muted);\n    margin:0 0 10px;\n  }\n\n  .solstar-list{\n    list-style:none;\n    padding:0;\n    margin:0;\n    display:grid;\n    gap:10px;\n  }\n\n  .solstar-li{\n    border:1px solid var(--line);\n    border-radius:14px;\n    background:#fff;\n    overflow:hidden;\n  }\n\n  .solstar-row{\n    display:flex;\n    align-items:center;\n    gap:12px;\n    padding:12px;\n  }\n\n  .solstar-item-text{\n    font-weight:700;\n    font-size:18px;\n    line-height:1.2;\n  }\n  @media (max-width:768px){\n    .solstar-item-text{ font-size:16px; }\n  }\n\n  /* ＋ button（黒統一） */\n  .solstar-plus{\n    width:34px;\n    height:34px;\n    border-radius:50%;\n    border:1px solid #000;\n    background:#fff;\n    display:flex;\n    align-items:center;\n    justify-content:center;\n    cursor:pointer;\n    flex-shrink:0;\n    padding:0;\n    transition:transform .25s ease;\n  }\n  .solstar-plus .plusmark{\n    position:relative;\n    width:14px;\n    height:14px;\n  }\n  .solstar-plus .plusmark::before,\n  .solstar-plus .plusmark::after{\n    content:\"\";\n    position:absolute;\n    left:50%;\n    top:50%;\n    background:#000; /* ← 黒 */\n    border-radius:2px;\n    transform:translate(-50%,-50%);\n  }\n  .solstar-plus .plusmark::before{\n    width:14px;\n    height:2px;\n  }\n  .solstar-plus .plusmark::after{\n    width:2px;\n    height:14px;\n  }\n\n  .solstar-li.is-open .solstar-plus{\n    transform:rotate(135deg);\n  }\n\n  /* accordion */\n  .solstar-detail{\n    max-height:0;\n    opacity:0;\n    overflow:hidden;\n    padding:0 12px 0 58px;\n    font-size:14px;\n    line-height:1.75;\n    color:#334155;\n    transition:max-height .35s ease, opacity .25s ease, padding-bottom .35s ease;\n  }\n  .solstar-li.is-open .solstar-detail{\n    max-height:240px;\n    opacity:1;\n    padding-bottom:12px;\n  }\n\n  /* X */\n  .solstar-connector{ display:none; }\n  @media (min-width:860px){\n    .solstar-connector{\n      display:flex;\n      align-items:center;\n      justify-content:center;\n    }\n    .solstar-connector-x{\n      font-size:56px;\n      font-weight:900;\n      color:#000;\n      line-height:1;\n    }\n  }\n\n  /* Partnership */\n  .solstar-partnership{\n    text-align:center;\n    font-weight:800;\n    font-size:20px;\n    margin:80px 0;\n  }\n  @media (max-width:768px){\n    .solstar-partnership{ font-size:18px; }\n  }\n\n  .solstar-partnership-image{\n    text-align:center;\n  }\n  .solstar-partnership-image img{\n    width:100%;\n    max-width:820px;\n    margin:0 auto;\n    display:block;\n    border-radius:14px;\n  }\n</style>\n\n<section class=\"solstar-role-wrap\" id=\"solstarRoleSection\">\n  <div class=\"solstar-role-inner\">\n\n    <div class=\"solstar-role-head\">\n      <h2 class=\"solstar-role-title\">SOLSTAR EXPRESSの役割分担</h2>\n    </div>\n\n    <div class=\"solstar-role-grid\">\n\n      <!-- SOLSTAR -->\n      <div class=\"solstar-card\">\n        <p class=\"solstar-card-title\">SOLSTARが対応すること</p>\n        <p class=\"solstar-card-desc\">ECサイトの設計・制作・運用</p>\n\n        <ul class=\"solstar-list\">\n          <li class=\"solstar-li\" data-detail=\"お客様の事業と商品を理解し、売れる導線を設計。ブランドの価値と世界観をUIで表現し、各ページを形にします。\">\n            <div class=\"solstar-row\">\n              <button class=\"solstar-plus\"><span class=\"plusmark\"></span></button>\n              <div class=\"solstar-item-text\">ECサイト制作</div>\n            </div>\n            <div class=\"solstar-detail\"></div>\n          </li>\n\n          <li class=\"solstar-li\" data-detail=\"バナーの差し替えや新規ページの追加、アプリの実装、商品登録など、日々発生する更新・修正にも柔軟に対応し、ECサイト運用を継続的にサポートします。\">\n            <div class=\"solstar-row\">\n              <button class=\"solstar-plus\"><span class=\"plusmark\"></span></button>\n              <div class=\"solstar-item-text\">サイト更新・修正</div>\n            </div>\n            <div class=\"solstar-detail\"></div>\n          </li>\n\n          <li class=\"solstar-li\" data-detail=\"売上データを中心にECサイトの状況を可視化し、数字をもとに次の改善につながる運用サポートとレポートを行います。\">\n            <div class=\"solstar-row\">\n              <button class=\"solstar-plus\"><span class=\"plusmark\"></span></button>\n              <div class=\"solstar-item-text\">運用サポート・レポート</div>\n            </div>\n            <div class=\"solstar-detail\"></div>\n          </li>\n        </ul>\n      </div>\n\n      <!-- X -->\n      <div class=\"solstar-connector\">\n        <div class=\"solstar-connector-x\">X</div>\n      </div>\n\n      <!-- CUSTOMER -->\n      <div class=\"solstar-card\">\n        <p class=\"solstar-card-title\">お客様に対応いただくこと</p>\n        <p class=\"solstar-card-desc\">商品とリアルオペレーション</p>\n\n        <ul class=\"solstar-list\">\n          <li class=\"solstar-li\" data-detail=\"ECサイト上の商品在庫の管理は、お客様にてご対応いただきます。\">\n            <div class=\"solstar-row\">\n              <button class=\"solstar-plus\"><span class=\"plusmark\"></span></button>\n              <div class=\"solstar-item-text\">商品の在庫管理</div>\n            </div>\n            <div class=\"solstar-detail\"></div>\n          </li>\n\n          <li class=\"solstar-li\" data-detail=\"注文情報は弊社から共有。その情報をもとに、スムーズな出荷・配送をご対応いただきます。\">\n            <div class=\"solstar-row\">\n              <button class=\"solstar-plus\"><span class=\"plusmark\"></span></button>\n              <div class=\"solstar-item-text\">出荷・発送オペレーション</div>\n            </div>\n            <div class=\"solstar-detail\"></div>\n          </li>\n\n          <li class=\"solstar-li\" data-detail=\"購入商品に関するお問い合わせから、クレーム・返品対応まで、エンドユーザー対応をご担当いただきます。\">\n            <div class=\"solstar-row\">\n              <button class=\"solstar-plus\"><span class=\"plusmark\"></span></button>\n              <div class=\"solstar-item-text\">カスタマーサポート</div>\n            </div>\n            <div class=\"solstar-detail\"></div>\n          </li>\n        </ul>\n      </div>\n\n    </div>\n\n    <div class=\"solstar-partnership\">\n      ECを一緒に育てるパートナーシップ。\n    </div>\n\n    <div class=\"solstar-partnership-image\">\n      <img src=\"https://cdn.shopify.com/s/files/1/0704/2642/7614/files/Solstar_x_Customer_Result_Image_copy.webp?v=1766713813\" alt=\"\">\n    </div>\n\n  </div>\n</section>\n\n<script>\n(function(){\n  const root = document.getElementById('solstarRoleSection');\n  if(!root) return;\n\n  root.addEventListener('click', e => {\n    const btn = e.target.closest('.solstar-plus');\n    if(!btn) return;\n\n    const item = btn.closest('.solstar-li');\n    if(!item) return;\n\n    const detail = item.querySelector('.solstar-detail');\n    if(detail && !detail.textContent.trim()){\n      detail.textContent = item.getAttribute('data-detail') || '';\n    }\n\n    root.querySelectorAll('.solstar-li.is-open').forEach(el=>{\n      if(el !== item) el.classList.remove('is-open');\n    });\n\n    item.classList.toggle('is-open');\n  });\n})();\n</script>",
