    "section_custom_code_8WDV9d": {
      "type": "section-custom-code",
      "name": "Custom code",
      "settings": {
        "code": "<style>\n  /* ===========================\n     SOLSTAR – Benefit Cards\n     =========================== */\n  .solstar-benefits-section {\n    background: #ffffff;\n    padding: 64px 16px;\n    color: #111827;\n  }\n\n  .solstar-benefits-inner {\n    max-width: 1260px;\n    margin: 0 auto;\n  }\n\n  .solstar-benefits-header {\n    text-align: left;\n    margin-bottom: 32px;\n  }\n\n  .solstar-benefits-header-label {\n    font-size: 12px;\n    letter-spacing: 0.16em;\n    text-transform: uppercase;\n    color: #6b7280;\n    margin-bottom: 8px;\n  }\n\n  .solstar-benefits-header-title {\n    font-size: 36px; /* セクション見出し */\n    font-weight: 700;\n    letter-spacing: 0.04em;\n    margin-bottom: 8px;\n  }\n\n  .solstar-benefits-header-desc {\n    font-size: 14px;\n    color: #4b5563;\n    line-height: 1.6;\n  }\n\n  /* ===========================\n     Cards Layout\n     =========================== */\n  .solstar-benefits-grid {\n    display: flex;\n    gap: 21px; /* 406 * 3 + 21 * 2 = 1260 */\n    justify-content: space-between;\n  }\n\n  @media (max-width: 900px) {\n    .solstar-benefits-grid {\n      flex-direction: column;\n      gap: 16px;\n    }\n  }\n\n  .solstar-benefit-card {\n    position: relative;\n    background: #ffffff;\n    border-radius: 20px;\n    border: 1px solid #e5e7eb;\n    cursor: pointer;\n    box-shadow: 0 10px 30px rgba(15, 23, 42, 0.04);\n    overflow: hidden;\n    transition:\n      transform 220ms ease,\n      box-shadow 220ms ease,\n      border-color 220ms ease,\n      background 220ms ease;\n    opacity: 0;\n    transform: translateY(24px);\n\n    width: 406px;\n    height: 257px;\n    box-sizing: border-box;\n\n    display: block; /* 中央寄せのflexは使わない */\n    text-align: center;\n  }\n\n  @media (max-width: 900px) {\n    .solstar-benefit-card {\n      width: 100%;\n      max-width: 406px;\n      margin: 0 auto;\n      height: 257px;\n    }\n  }\n\n  .solstar-benefit-card::before {\n    content: \"\";\n    position: absolute;\n    inset: 0;\n    background: radial-gradient(\n      circle at top left,\n      rgba(20, 78, 148, 0.12),\n      transparent 60%\n    );\n    opacity: 0;\n    transition: opacity 220ms ease;\n    pointer-events: none;\n  }\n\n  /* Hover Zoom（is-visible と共存させる） */\n  .solstar-benefit-card.is-visible:hover {\n    transform: translateY(-4px) scale(1.03);\n    box-shadow: 0 20px 50px rgba(15, 23, 42, 0.12);\n    border-color: #d1d5db;\n    background: #f9fafb;\n  }\n\n  .solstar-benefit-card.is-visible:hover::before {\n    opacity: 1;\n  }\n\n  .solstar-benefit-card:active {\n    transform: translateY(0) scale(1);\n    box-shadow: 0 10px 30px rgba(15, 23, 42, 0.04);\n  }\n\n  /* POINT 01 / 02 / 03 をカード上部に固定（上中央） */\n  .solstar-benefit-tag {\n    position: absolute;\n    top: 16px;\n    left: 50%;\n    transform: translateX(-50%);\n    font-size: 11px;\n    font-weight: 600;\n    letter-spacing: 0.14em;\n    text-transform: uppercase;\n    color: #6b7280;\n    margin: 0;\n    text-align: center;\n  }\n\n  /* ===========================\n     見出しブロック（高さガチ統一）\n     =========================== */\n  .solstar-benefit-heading {\n    margin: 0 auto;\n    width: 100%;\n    max-width: 320px;\n\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    justify-content: center;\n\n    height: 120px;           /* ★3枚とも同じ高さで完全固定 */\n    text-align: center;\n  }\n\n  .solstar-benefit-heading-main {\n    display: block;\n    font-size: 36px;         /* 早さ / 安さ / 検証しやすさ */\n    font-weight: 700;\n    line-height: 1.1;\n    margin-bottom: 10px;     /* メイン行とサブ行の間に余白 */\n  }\n\n  .solstar-benefit-heading-sub {\n    display: block;\n    font-size: 24px;         /* サブコピー */\n    font-weight: 700;\n    line-height: 1.4;\n  }\n\n  /* カード右下の◯×アイコン */\n  .solstar-benefit-open {\n    position: absolute;\n    right: 16px;\n    bottom: 14px;\n  }\n\n  .solstar-benefit-open-icon {\n    width: 28px;\n    height: 28px;\n    border-radius: 999px;\n    border: 1px solid #d1d5db;\n    display: inline-flex;\n    align-items: center;\n    justify-content: center;\n    font-size: 16px;\n    color: #6b7280;\n    background: #ffffff;\n    pointer-events: none; /* クリック領域はbutton全体 */\n  }\n\n  .solstar-benefit-card.is-visible:hover .solstar-benefit-open-icon {\n    background: #e5eff9;\n    border-color: #bfdbfe;\n    color: #1f2937;\n  }\n\n  /* Scroll-in animation */\n  .solstar-benefit-card.is-visible {\n    opacity: 1;\n    transform: translateY(0);\n    transition:\n      opacity 480ms cubic-bezier(0.22, 0.61, 0.36, 1),\n      transform 480ms cubic-bezier(0.22, 0.61, 0.36, 1),\n      box-shadow 220ms ease,\n      border-color 220ms ease,\n      background 220ms ease;\n  }\n\n  /* ===========================\n     Modal\n     =========================== */\n  .solstar-benefit-modal-backdrop {\n    position: fixed;\n    inset: 0;\n    background: rgba(15, 23, 42, 0.36);\n    display: none;\n    align-items: center;\n    justify-content: center;\n    padding: 24px;\n    z-index: 9999;\n  }\n\n  .solstar-benefit-modal-backdrop.is-open {\n    display: flex;\n  }\n\n  .solstar-benefit-modal {\n    background: #ffffff;\n    border-radius: 20px;\n    max-width: 640px;\n    width: 100%;\n    box-shadow: 0 30px 80px rgba(15, 23, 42, 0.18);\n    position: relative;\n    padding: 26px 24px 24px;\n    transform: translateY(12px) scale(0.98);\n    opacity: 0;\n    animation: solstarModalIn 260ms cubic-bezier(0.22, 0.61, 0.36, 1) forwards;\n  }\n\n  @keyframes solstarModalIn {\n    from {\n      transform: translateY(16px) scale(0.96);\n      opacity: 0;\n    }\n    to {\n      transform: translateY(0) scale(1);\n      opacity: 1;\n    }\n  }\n\n  .solstar-benefit-modal-close {\n    position: absolute;\n    top: 14px;\n    right: 14px;\n    width: 28px;\n    height: 28px;\n    border-radius: 999px;\n    border: 1px solid #e5e7eb;\n    background: #f9fafb;\n    display: inline-flex;\n    align-items: center;\n    justify-content: center;\n    cursor: pointer;\n    font-size: 16px;\n    color: #4b5563;\n    transition:\n      background 160ms ease,\n      transform 160ms ease,\n      box-shadow 160ms ease;\n  }\n\n  .solstar-benefit-modal-close:hover {\n    background: #e5e7eb;\n    box-shadow: 0 8px 24px rgba(15, 23, 42, 0.12);\n    transform: translateY(-1px);\n  }\n\n  .solstar-benefit-modal-close:active {\n    transform: translateY(0);\n    box-shadow: none;\n  }\n\n  .solstar-benefit-modal-label {\n    font-size: 11px;\n    letter-spacing: 0.16em;\n    text-transform: uppercase;\n    color: #9ca3af;\n    margin-bottom: 8px;\n  }\n\n  .solstar-benefit-modal-title {\n    font-size: 18px;\n    font-weight: 700;\n    margin-bottom: 10px;\n  }\n\n  .solstar-benefit-modal-body {\n    font-size: 14px;\n    line-height: 1.7;\n    color: #4b5563;\n  }\n\n  .solstar-benefit-modal-body p {\n    margin-bottom: 10px;\n  }\n\n  .solstar-benefit-modal-body ul {\n    margin: 8px 0 12px;\n    padding-left: 18px;\n  }\n\n  .solstar-benefit-modal-body li {\n    margin-bottom: 4px;\n  }\n\n  .solstar-benefit-modal-footnote {\n    font-size: 11px;\n    color: #9ca3af;\n    margin-top: 10px;\n  }\n\n  @media (max-width: 600px) {\n    .solstar-benefit-modal-backdrop {\n      padding: 16px;\n    }\n    .solstar-benefit-modal {\n      padding: 22px 18px 18px;\n      border-radius: 18px;\n    }\n    .solstar-benefit-modal-title {\n      font-size: 16px;\n    }\n  }\n</style>\n\n\n<div class=\"solstar-benefits-section\">\n  <div class=\"solstar-benefits-inner\">\n    <div class=\"solstar-benefits-header\">\n      <div class=\"solstar-benefits-header-label\">SOLSTAR EXPRESS</div>\n      <h3 class=\"solstar-benefits-header-title\">\n        小規模事業にフィットする3つの強み\n      </h3>\n      <p class=\"solstar-benefits-header-desc\">\n        スピード・コスト・検証性。小さくはじめて、素早く試しながら育てていけるEC基盤です。\n      </p>\n    </div>\n\n    <div class=\"solstar-benefits-grid\">\n      <!-- Card 1 -->\n      <button\n        class=\"solstar-benefit-card\"\n        type=\"button\"\n        data-solstar-modal-target=\"#solstar-benefit-modal-1\"\n      >\n        <div class=\"solstar-benefit-tag\">POINT 01</div>\n        <h4 class=\"solstar-benefit-heading\">\n          <span class=\"solstar-benefit-heading-main\">早さ</span>\n          <span class=\"solstar-benefit-heading-sub\"\n            >スピード勝負の小規模事業に最適</span\n          >\n        </h4>\n        <div class=\"solstar-benefit-open\">\n          <span class=\"solstar-benefit-open-icon\">×</span>\n        </div>\n      </button>\n\n      <!-- Card 2 -->\n      <button\n        class=\"solstar-benefit-card\"\n        type=\"button\"\n        data-solstar-modal-target=\"#solstar-benefit-modal-2\"\n      >\n        <div class=\"solstar-benefit-tag\">POINT 02</div>\n        <h4 class=\"solstar-benefit-heading\">\n          <span class=\"solstar-benefit-heading-main\">安さ</span>\n          <span class=\"solstar-benefit-heading-sub\"\n            >コストをかけずに確かな効果を狙える</span\n          >\n        </h4>\n        <div class=\"solstar-benefit-open\">\n          <span class=\"solstar-benefit-open-icon\">×</span>\n        </div>\n      </button>\n\n      <!-- Card 3 -->\n      <button\n        class=\"solstar-benefit-card\"\n        type=\"button\"\n        data-solstar-modal-target=\"#solstar-benefit-modal-3\"\n      >\n        <div class=\"solstar-benefit-tag\">POINT 03</div>\n        <h4 class=\"solstar-benefit-heading\">\n          <span class=\"solstar-benefit-heading-main\">検証しやすさ</span>\n          <span class=\"solstar-benefit-heading-sub\"\n            >試せる、変えられる、やめられる</span\n          >\n        </h4>\n        <div class=\"solstar-benefit-open\">\n          <span class=\"solstar-benefit-open-icon\">×</span>\n        </div>\n      </button>\n    </div>\n  </div>\n</div>\n\n<!-- Modal 1 -->\n<div\n  class=\"solstar-benefit-modal-backdrop\"\n  id=\"solstar-benefit-modal-1\"\n  aria-hidden=\"true\"\n>\n  <div class=\"solstar-benefit-modal\" role=\"dialog\" aria-modal=\"true\">\n    <button\n      class=\"solstar-benefit-modal-close\"\n      type=\"button\"\n      data-solstar-modal-close\n      aria-label=\"閉じる\"\n    >\n      ×\n    </button>\n    <div class=\"solstar-benefit-modal-label\">POINT 01</div>\n    <div class=\"solstar-benefit-modal-title\">\n      早さ ー スピード勝負の小規模事業に最適\n    </div>\n    <div class=\"solstar-benefit-modal-body\">\n      <p>最短 2〜4週間で公開可能。</p>\n      <p>\n        新商品のリリースタイミングやクラウドファンディング後の流れを逃さず、すぐに販売開始できます。\n      </p>\n      <p>\n        Shopifyをベースに、必要十分な設計を施したECサイトをスピーディーにご提供します。\n      </p>\n    </div>\n  </div>\n</div>\n\n<!-- Modal 2 -->\n<div\n  class=\"solstar-benefit-modal-backdrop\"\n  id=\"solstar-benefit-modal-2\"\n  aria-hidden=\"true\"\n>\n  <div class=\"solstar-benefit-modal\" role=\"dialog\" aria-modal=\"true\">\n    <button\n      class=\"solstar-benefit-modal-close\"\n      type=\"button\"\n      data-solstar-modal-close\n      aria-label=\"閉じる\"\n    >\n      ×\n    </button>\n    <div class=\"solstar-benefit-modal-label\">POINT 02</div>\n    <div class=\"solstar-benefit-modal-title\">\n      安さ ー コストをかけずに確かな効果を狙える\n    </div>\n    <div class=\"solstar-benefit-modal-body\">\n      <p>\n        月額29,800円〜のサブスク型。初期費用も5万円と業界最安クラスです。\n      </p>\n      <p>不要な開発を省きつつ、初期セットには以下がすべて含まれます。</p>\n      <ul>\n        <li>ホームページ</li>\n        <li>商品ページ</li>\n        <li>LP（1本）</li>\n        <li>運営サポート（更新・設定・不具合一次対応・軽微なデザイン改修）</li>\n      </ul>\n      <p>低コストでも、見た目も使いやすさも高品質な設計です。</p>\n    </div>\n  </div>\n</div>\n\n<!-- Modal 3 -->\n<div\n  class=\"solstar-benefit-modal-backdrop\"\n  id=\"solstar-benefit-modal-3\"\n  aria-hidden=\"true\"\n>\n  <div class=\"solstar-benefit-modal\" role=\"dialog\" aria-modal=\"true\">\n    <button\n      class=\"solstar-benefit-modal-close\"\n      type=\"button\"\n      data-solstar-modal-close\n      aria-label=\"閉じる\"\n    >\n      ×\n    </button>\n    <div class=\"solstar-benefit-modal-label\">POINT 03</div>\n    <div class=\"solstar-benefit-modal-title\">\n      検証しやすさ ー 試せる、変えられる、やめられる\n    </div>\n    <div class=\"solstar-benefit-modal-body\">\n      <p>\n        SOLSTAR EXPRESSは、少数SKU・スモールスタート前提のサービス。\n        「検証しながら運用できるのが便利」とご好評をいただいています。\n      </p>\n      <ul>\n        <li>機能追加やデザイン強化も柔軟に対応可能</li>\n        <li>不要になった場合は解約も自由</li>\n        <li>成果が出たらそのまま本格運用にも拡張OK</li>\n      </ul>\n      <p>小規模事業に最適な、フレキシブルなEC基盤です。</p>\n      <div class=\"solstar-benefit-modal-footnote\">\n        ※ 最短契約期間は1年間\n      </div>\n    </div>\n  </div>\n</div>\n\n<script>\n  (function () {\n    // ===== Scroll-in animation for cards =====\n    var cards = document.querySelectorAll(\".solstar-benefit-card\");\n\n    if (\"IntersectionObserver\" in window) {\n      var observer = new IntersectionObserver(\n        function (entries) {\n          entries.forEach(function (entry) {\n            if (entry.isIntersecting) {\n              entry.target.classList.add(\"is-visible\");\n              observer.unobserve(entry.target);\n            }\n          });\n        },\n        {\n          threshold: 0.3,\n        }\n      );\n\n      cards.forEach(function (card) {\n        observer.observe(card);\n      });\n    } else {\n      // Fallback: show all if IntersectionObserver not supported\n      cards.forEach(function (card) {\n        card.classList.add(\"is-visible\");\n      });\n    }\n\n    // ===== Modal logic =====\n    var openButtons = document.querySelectorAll(\n      \"[data-solstar-modal-target]\"\n    );\n    var closeButtons = document.querySelectorAll(\n      \"[data-solstar-modal-close]\"\n    );\n\n    function openModal(id) {\n      var modal = document.querySelector(id);\n      if (!modal) return;\n      modal.classList.add(\"is-open\");\n      modal.setAttribute(\"aria-hidden\", \"false\");\n      document.documentElement.style.overflow = \"hidden\";\n    }\n\n    function closeModal(modal) {\n      modal.classList.remove(\"is-open\");\n      modal.setAttribute(\"aria-hidden\", \"true\");\n      document.documentElement.style.overflow = \"\";\n    }\n\n    openButtons.forEach(function (btn) {\n      btn.addEventListener(\"click\", function () {\n        var target = btn.getAttribute(\"data-solstar-modal-target\");\n        if (!target) return;\n        openModal(target);\n      });\n    });\n\n    closeButtons.forEach(function (btn) {\n      btn.addEventListener(\"click\", function () {\n        var modal = btn.closest(\".solstar-benefit-modal-backdrop\");\n        if (modal) closeModal(modal);\n      });\n    });\n\n    // click on backdrop to close\n    document.addEventListener(\"click\", function (e) {\n      var backdrop = e.target.closest(\".solstar-benefit-modal-backdrop\");\n      var modalContent = e.target.closest(\".solstar-benefit-modal\");\n      if (backdrop && !modalContent && backdrop.classList.contains(\"is-open\")) {\n        closeModal(backdrop);\n      }\n    });\n\n    // ESC key to close\n    document.addEventListener(\"keydown\", function (e) {\n      if (e.key === \"Escape\") {\n        document\n          .querySelectorAll(\".solstar-benefit-modal-backdrop.is-open\")\n          .forEach(function (modal) {\n            closeModal(modal);\n          });\n      }\n    });\n  })();\n</script>",
        "width": "wrapper--full",
        "color_scheme": "scheme-1",
        "padding_top": 0,
        "padding_bottom": 0
      }
