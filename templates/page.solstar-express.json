    "section_custom_code_8L7hhp": {
      "type": "section-custom-code",
      "name": "Custom code",
      "settings": {
        "code": "<style>\n  /* ========== SOLSTAR - Case Study Slider (No CASE#, No Dot) ========== */\n  .solstar-case-section {\n    position: relative;\n    background: #ffffff;\n    color: #111827;\n    padding: 64px 16px;\n    overflow: hidden;\n    opacity: 0;\n    transform: translateY(30px);\n    transition:\n      opacity 0.6s ease-out,\n      transform 0.6s ease-out;\n  }\n\n  .solstar-case-section.is-visible {\n    opacity: 1;\n    transform: translateY(0);\n  }\n\n  .solstar-case-inner {\n    max-width: 960px;\n    margin: 0 auto;\n  }\n\n  /* 見出し 24px */\n  .solstar-case-heading {\n    font-size: 24px;\n    font-weight: 700;\n    letter-spacing: 0.04em;\n    margin: 0 0 12px;\n  }\n\n  /* ※ リード文はHTMLから削除済み。クラスは残しておいてもOK */\n  .solstar-case-lead {\n    font-size: 14px;\n    line-height: 1.7;\n    color: #4b5563;\n    margin: 0 0 32px;\n  }\n\n  /* コンテナ：白ベース＋枠のみ（上部の青いライン削除済み） */\n  .solstar-case-slider {\n    position: relative;\n    width: 100%;\n    border-radius: 20px;\n    background: #ffffff;\n    border: 1px solid #e5e7eb;\n    padding: 28px 20px 40px;\n    box-shadow: 0 14px 35px rgba(15, 23, 42, 0.12);\n    color: #111827;\n    overflow: hidden;\n  }\n\n  @media (min-width: 768px) {\n    .solstar-case-slider {\n      padding: 32px 40px 44px;\n    }\n  }\n\n  /* スライドラッパ：高さはJSで最大値を固定セット */\n  .solstar-case-slides {\n    position: relative;\n    width: 100%;\n    min-height: 260px; /* 高さ潰れ防止の保険 */\n  }\n\n  .solstar-case-slide {\n    position: absolute;\n    inset: 0;\n    opacity: 0;\n    transform: translateX(40px);\n    transition:\n      opacity 0.5s ease,\n      transform 0.5s ease;\n    pointer-events: none;\n  }\n\n  /* アクティブスライド */\n  .solstar-case-slide.is-active {\n    opacity: 1;\n    transform: translateX(0);\n    pointer-events: auto;\n  }\n\n  /* タイトルのみ（ラベルは削除） */\n  .solstar-case-title {\n    font-size: 24px;\n    font-weight: 700;\n    margin: 4px 0 10px;\n    color: #111827;\n  }\n\n  /* テキストは14pxで統一 */\n  .solstar-case-sub {\n    font-size: 14px;\n    color: #4b5563;\n    margin: 0 0 16px;\n  }\n\n  .solstar-case-body {\n    font-size: 14px;\n    line-height: 1.8;\n    color: #111827;\n    margin: 0 0 16px;\n  }\n\n  .solstar-case-quote-block {\n    border-left: 3px solid rgba(20, 78, 148, 0.3);\n    padding-left: 14px;\n    margin-top: 8px;\n  }\n\n  .solstar-case-quote-label {\n    font-size: 11px;\n    letter-spacing: 0.12em;\n    text-transform: uppercase;\n    color: #9ca3af;\n    margin-bottom: 4px;\n  }\n\n  .solstar-case-quote {\n    font-size: 14px;\n    font-style: italic;\n    color: #111827;\n  }\n\n  /* 万が一テーマ側で label/dot が残ってても強制で非表示にする */\n  .solstar-case-label,\n  .solstar-case-label-dot {\n    display: none !important;\n    width: 0 !important;\n    height: 0 !important;\n    padding: 0 !important;\n    margin: 0 !important;\n  }\n  .solstar-case-label::before,\n  .solstar-case-label::after,\n  .solstar-case-label-dot::before,\n  .solstar-case-label-dot::after {\n    content: none !important;\n  }\n\n  /* コントロール：テキストの “下” に配置（重ならない） */\n  .solstar-case-controls {\n    margin-top: 24px;\n    display: flex;\n    align-items: center;\n    justify-content: space-between;\n    gap: 16px;\n  }\n\n  .solstar-case-arrows {\n    display: flex;\n    gap: 8px;\n  }\n\n  .solstar-case-arrow-btn {\n    width: 36px;\n    height: 36px;\n    border-radius: 999px;\n    border: 1px solid #d1d5db;\n    background: #ffffff;\n    display: inline-flex;\n    align-items: center;\n    justify-content: center;\n    cursor: pointer;\n    padding: 0;\n    transition:\n      background 0.2s ease,\n      transform 0.15s ease,\n      border-color 0.2s ease;\n  }\n\n  .solstar-case-arrow-btn span {\n    display: inline-block;\n    font-size: 16px;\n    line-height: 1;\n    color: #111827;\n  }\n\n  .solstar-case-arrow-btn:hover {\n    background: #144e94;\n    border-color: #144e94;\n  }\n\n  .solstar-case-arrow-btn:hover span {\n    color: #ffffff;\n  }\n\n  .solstar-case-arrow-btn:active {\n    transform: translateY(0);\n  }\n\n  .solstar-case-dots {\n    display: flex;\n    gap: 8px;\n    align-items: center;\n  }\n\n  .solstar-case-dot {\n    width: 7px;\n    height: 7px;\n    border-radius: 999px;\n    background: #d1d5db;\n    cursor: pointer;\n    transition:\n      background 0.2s ease,\n      transform 0.2s ease,\n      width 0.2s ease;\n  }\n\n  .solstar-case-dot.is-active {\n    width: 18px;\n    background: #144e94;\n    transform: translateY(-1px);\n  }\n\n  .solstar-case-counter {\n    font-size: 11px;\n    color: #6b7280;\n  }\n\n  @media (max-width: 767px) {\n    .solstar-case-controls {\n      flex-direction: column-reverse;\n      align-items: flex-start;\n      gap: 10px;\n    }\n  }\n</style>\n\n<div class=\"solstar-case-section\" id=\"solstar-case-section\">\n  <div class=\"solstar-case-inner\">\n    <div class=\"solstar-case-heading\">Shopify ECサイト構築実績</div>\n\n    <!-- リード文は削除 -->\n\n    <div class=\"solstar-case-slider\" data-solstar-case-slider>\n      <div class=\"solstar-case-slides\">\n        <!-- Slide 1: ONETREE -->\n        <div class=\"solstar-case-slide is-active\">\n          <div class=\"solstar-case-title\">株式会社ONETREE様</div>\n          <p class=\"solstar-case-sub\">\n            1秒で履けるスニーカー「LAQUN」の公式ECサイト構築\n          </p>\n          <p class=\"solstar-case-body\">\n            1秒で履けるスニーカー「LAQUN」を製造・販売されている株式会社ONETREE様。公式ECサイトを約2か月で立ち上げ、公開初月に12万円以上の売上を達成されました。Shopifyを活用し、ブランドの認知拡大とファンづくりを進められています。\n          </p>\n          <div class=\"solstar-case-quote-block\">\n            <div class=\"solstar-case-quote-label\">ご担当者様の声</div>\n            <div class=\"solstar-case-quote\">\n              “説明も丁寧で、細かな調整にも対応していただきました。ストアにしっかり反映してもらえて満足しています。”\n            </div>\n          </div>\n        </div>\n\n        <!-- Slide 2: EDGBLACK -->\n        <div class=\"solstar-case-slide\">\n          <div class=\"solstar-case-title\">株式会社EDGBLACK様</div>\n          <p class=\"solstar-case-sub\">\n            高品質な砥石ブランドのEC基盤を既存プラットフォームから移行\n          </p>\n          <p class=\"solstar-case-body\">\n            誰でも簡単にプロ級の刃物研ぎができる、高品質な砥石とアクセサリを展開されている株式会社EDGBLACK様。既存プラットフォームからShopifyへの移行を支援し、公開初月には27万円以上の売上を実現されました。現在もスタイリッシュで使いやすいECサイトとして継続的に運用されています。\n          </p>\n          <div class=\"solstar-case-quote-block\">\n            <div class=\"solstar-case-quote-label\">ご担当者様の声</div>\n            <div class=\"solstar-case-quote\">\n              “とても丁寧かつ詳細に説明してくださったので、困ることは一切ありませんでした。”\n            </div>\n          </div>\n        </div>\n\n        <!-- Slide 3: Rsworks -->\n        <div class=\"solstar-case-slide\">\n          <div class=\"solstar-case-title\">合同会社Rsworks様</div>\n          <p class=\"solstar-case-sub\">\n            鍛冶の匠 ─ 日本の技を受け継ぐ包丁ブランド\n          </p>\n          <p class=\"solstar-case-body\">\n            モール型ECサイトからの脱却を検討していたものの、専任の担当者が不在で、自社ECの運営が思うように進んでいなかった合同会社Rsworks様。高級感を保ちながら、ブランドの世界観とイメージをしっかり訴求できるECサイトを構築しました。ECサイト立ち上げキャンペーンを実施し、驚異のLINE登録単価30円を達成。キャンペーン期間中にLINE登録者2,306人、Instagramフォロワー950人を獲得しました。\n          </p>\n          <div class=\"solstar-case-quote-block\">\n            <div class=\"solstar-case-quote-label\">ご担当者様の声</div>\n            <div class=\"solstar-case-quote\">\n              「洗練されたクリエイティブセンスで一目置くECサイトを構築していただき、とても満足しています。」\n            </div>\n          </div>\n        </div>\n\n        <!-- Slide 4: PAW MINT -->\n        <div class=\"solstar-case-slide\">\n          <div class=\"solstar-case-title\">PAW MINT様</div>\n          <p class=\"solstar-case-sub\">\n            愛犬との「時間」を大切にするペットブランド\n          </p>\n          <p class=\"solstar-case-body\">\n            ペットの日本市場に向けて、新しい形の商品をどのように打ち出すかを模索されていたPAW MINT様。愛おしい愛犬への愛情を、ポップで楽しい世界観としてサイト全体に表現しました。新商品の魅力を最大限に伝えるため、専用ランディングページを制作し、広告施策により2,000名を超えるメルマガ登録者を獲得しました。\n          </p>\n          <div class=\"solstar-case-quote-block\">\n            <div class=\"solstar-case-quote-label\">ご担当者様の声</div>\n            <div class=\"solstar-case-quote\">\n              「可愛くポップなECサイトを作って頂きました。」\n            </div>\n          </div>\n        </div>\n\n        <!-- Slide 5: SANDY HAZE -->\n        <div class=\"solstar-case-slide\">\n          <div class=\"solstar-case-title\">SANDY HAZE様</div>\n          <p class=\"solstar-case-sub\">\n            自由と美しさをまとう女性のための水着ブランド\n          </p>\n          <p class=\"solstar-case-body\">\n            英語圏向けの越境ECサイトを構築し売上を拡大したいものの、担当者が不在のためプロジェクトが進行していなかったSANDY HAZE様。視覚的な魅力を引き出す、デザイン重視のサイト制作に注力しました。アメリカ、カナダ、オーストラリアの女性層をターゲットにした広告キャンペーンを展開し、サイト立ち上げからわずか3ヶ月で月商100万円を達成しました。\n          </p>\n          <div class=\"solstar-case-quote-block\">\n            <div class=\"solstar-case-quote-label\">ご担当者様の声</div>\n            <div class=\"solstar-case-quote\">\n              “They made our first e-commerce site look amazing.”\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <div class=\"solstar-case-controls\">\n        <div class=\"solstar-case-arrows\">\n          <button\n            type=\"button\"\n            class=\"solstar-case-arrow-btn\"\n            data-solstar-case-prev\n            aria-label=\"前の事例へ\"\n          >\n            <span>&larr;</span>\n          </button>\n          <button\n            type=\"button\"\n            class=\"solstar-case-arrow-btn\"\n            data-solstar-case-next\n            aria-label=\"次の事例へ\"\n          >\n            <span>&rarr;</span>\n          </button>\n        </div>\n\n        <div style=\"display: flex; align-items: center; gap: 12px;\">\n          <div class=\"solstar-case-dots\" data-solstar-case-dots></div>\n          <div class=\"solstar-case-counter\" data-solstar-case-counter></div>\n        </div>\n      </div>\n    </div>\n  </div>\n</div>\n\n<script>\n  (function () {\n    var slider = document.querySelector(\"[data-solstar-case-slider]\");\n    if (!slider) return;\n\n    var slides = Array.prototype.slice.call(\n      slider.querySelectorAll(\".solstar-case-slide\")\n    );\n    var slidesContainer = slider.querySelector(\".solstar-case-slides\");\n    var dotsContainer = slider.querySelector(\"[data-solstar-case-dots]\");\n    var counter = slider.querySelector(\"[data-solstar-case-counter]\");\n    var prevBtn = slider.querySelector(\"[data-solstar-case-prev]\");\n    var nextBtn = slider.querySelector(\"[data-solstar-case-next]\");\n    var current = 0;\n    var autoTimer = null;\n    var AUTO_INTERVAL = 7000; // 7秒ごとに自動スライド\n\n    /* ===== 高さを最大スライドに合わせて固定（絶対配置でも正しく計測） ===== */\n    function calculateMaxHeight() {\n      if (!slidesContainer || !slides.length) return;\n\n      var maxHeight = 0;\n\n      slides.forEach(function (slide) {\n        // 現状のスタイルを保存\n        var prevOpacity = slide.style.opacity;\n        var prevTransform = slide.style.transform;\n        var prevPosition = slide.style.position;\n        var prevPointer = slide.style.pointerEvents;\n\n        // 一時的に「通常フロー」にして高さ計測\n        slide.style.position = \"relative\";\n        slide.style.opacity = \"1\";\n        slide.style.transform = \"none\";\n        slide.style.pointerEvents = \"auto\";\n\n        var h = slide.offsetHeight;\n        if (h > maxHeight) maxHeight = h;\n\n        // 元に戻す\n        slide.style.position = prevPosition;\n        slide.style.opacity = prevOpacity;\n        slide.style.transform = prevTransform;\n        slide.style.pointerEvents = prevPointer;\n      });\n\n      if (maxHeight > 0) {\n        slidesContainer.style.height = maxHeight + \"px\";\n      }\n    }\n\n    // 初期計測\n    calculateMaxHeight();\n\n    // リサイズ時も再計測（簡易デバウンス）\n    var resizeTimer;\n    window.addEventListener(\"resize\", function () {\n      clearTimeout(resizeTimer);\n      resizeTimer = setTimeout(calculateMaxHeight, 200);\n    });\n\n    /* ===== Dots生成 ===== */\n    slides.forEach(function (_, index) {\n      var dot = document.createElement(\"button\");\n      dot.type = \"button\";\n      dot.className = \"solstar-case-dot\" + (index === 0 ? \" is-active\" : \"\");\n      dot.setAttribute(\"aria-label\", \"事例 \" + (index + 1) + \" に切り替え\");\n      dot.addEventListener(\"click\", function () {\n        goToSlide(index);\n        restartAuto();\n      });\n      dotsContainer.appendChild(dot);\n    });\n\n    var dots = Array.prototype.slice.call(\n      dotsContainer.querySelectorAll(\".solstar-case-dot\")\n    );\n\n    function updateCounter() {\n      if (!counter) return;\n      counter.textContent = current + 1 + \" / \" + slides.length;\n    }\n\n    function goToSlide(index) {\n      if (index === current || index < 0 || index >= slides.length) return;\n\n      slides[current].classList.remove(\"is-active\");\n      dots[current].classList.remove(\"is-active\");\n\n      current = index;\n\n      slides[current].classList.add(\"is-active\");\n      dots[current].classList.add(\"is-active\");\n      updateCounter();\n    }\n\n    function nextSlide() {\n      var target = (current + 1) % slides.length;\n      goToSlide(target);\n    }\n\n    function prevSlide() {\n      var target = (current - 1 + slides.length) % slides.length;\n      goToSlide(target);\n    }\n\n    function startAuto() {\n      if (autoTimer) return;\n      autoTimer = setInterval(nextSlide, AUTO_INTERVAL);\n    }\n\n    function stopAuto() {\n      if (!autoTimer) return;\n      clearInterval(autoTimer);\n      autoTimer = null;\n    }\n\n    function restartAuto() {\n      stopAuto();\n      startAuto();\n    }\n\n    if (nextBtn) {\n      nextBtn.addEventListener(\"click\", function () {\n        nextSlide();\n        restartAuto();\n      });\n    }\n\n    if (prevBtn) {\n      prevBtn.addEventListener(\"click\", function () {\n        prevSlide();\n        restartAuto();\n      });\n    }\n\n    // ホバー中は自動スライド停止（PC向け）\n    slider.addEventListener(\"mouseenter\", stopAuto);\n    slider.addEventListener(\"mouseleave\", startAuto);\n\n    updateCounter();\n    startAuto();\n  })();\n\n  // セクションのスクロールイン演出\n  (function () {\n    var section = document.getElementById(\"solstar-case-section\");\n    if (!section) return;\n\n    if (\"IntersectionObserver\" in window) {\n      var observer = new IntersectionObserver(\n        function (entries) {\n          entries.forEach(function (entry) {\n            if (entry.isIntersecting) {\n              section.classList.add(\"is-visible\");\n              observer.unobserve(section);\n            }\n          });\n        },\n        { threshold: 0.2 }\n      );\n      observer.observe(section);\n    } else {\n      section.classList.add(\"is-visible\");\n    }\n  })();\n</script>",
